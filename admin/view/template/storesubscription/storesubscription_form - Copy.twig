{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-filter" name="togglee" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        
      </ul>
    </div>
  </div>
  <div class="container-fluid"> {% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-pencil"></i> {{text_form}}</h3>
      </div>
      <div class="panel-body">
        <form action="{{delete}}" method="post" enctype="multipart/form-data" id="form-filter" class="form-horizontal">
         <div class="form-group">
            <label class="col-sm-2 control-label" for="input-store">{{ entry_name }}</label>
            <div class="col-sm-6">
              <select name="store_id" id="input-store" class="form-control">
              <option value="0">{{ text_select }}</option>
                {% for store in stores %}
                {% if store.store_id == store_id %}
                <option value="{{ store.store_id }}" selected="selected">{{ store.name }}</option>
                {% else %}
                <option value="{{ store.store_id }}">{{ store.name }}</option>
                {% endif %}
                {% endfor %}
              </select>
            </div>
          </div>

               <div class="form-group">
                <label class="col-sm-2 control-label" for="input-payment">{{ entry_card }}</label>
                <div class="col-sm-6">
                  <input type="text" name="commission_percentage_card" value="{{ commission_percentage_card }}" placeholder="{{ entry_card }}" id="input-payment" class="form-control" />
                  
                </div>
              </div>

                <div class="form-group">
                <label class="col-sm-2 control-label" for="input-cash">{{ entry_cod }}</label>
                <div class="col-sm-6">
                  <input type="text" name="commission_percentage_cod" value="{{ commission_percentage_cod }}" placeholder="{{ entry_cod }}" id="input-cash" class="form-control" />
                  
                </div>
              </div>


          <div class="form-group">
                <label class="col-sm-2 control-label" for="input-tax-class">{{ entry_gst }}</label>
                <div class="col-sm-6">
                  <select name="GST_commission_percentage" id="input-tax-class" class="form-control">
                    <option value="0">{{ text_none }}</option>


                    {% for tax_class in tax_classes %}
                      {% if tax_class.tax_class_id == tax_class_id %}


                        <option value="{{ tax_class.tax_class_id }}" selected="selected">{{ tax_class.title }}</option>


                      {% else %}


                        <option value="{{ tax_class.tax_class_id }}">{{ tax_class.title }}</option>


                      {% endif %}
                    {% endfor %}


                  </select>
                </div>
              </div>

               

          
        
        </form>
      </div>
    </div>
  </div>
  

{{ footer }} 















 